server {  
    listen 80 default_server;  
    listen [::]:80 default_server;  
    root /var/www/sites/test-qualite/public;  
    index index.php index.html index.htm;  
    server_name _;  
    location / {  
        try_files $uri $uri/ =404;  
    }  
    location ~ .php$ {  
        include snippets/fastcgi-php.conf;  
        fastcgi_pass unix:/var/run/php/php7.7-fpm.sock;  
    }  
}
